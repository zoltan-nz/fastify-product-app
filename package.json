{
  "name": "node-express-typescript2",
  "version": "1.0.0",
  "description": "TypeScript2, Node.js, Express Starter Kit",
  "scripts": {
    "clean": "rimraf lib .nyc-output coverage",
    "lint": "tslint --force --fix \"src/**/*.ts\" \"test/**/*.ts\"",
    "prebuild": "yarn lint",
    "build": "yarn run clean && echo Using TypeScript && tsc --version && tsc --pretty",
    "pretest": "yarn build",
    "test": "mocha",
    "testOnly": "mocha",
    "test:coverage": "nyc yarn test && open coverage/index.html",
    "test:watch": "yarn test -- --watch",
    "prepare": "yarn test",
    "watch": "yarn build -- --watch",
    "server": "nodemon ./dist/bin/www",
    "prestart": "yarn build",
    "start": "node ./dist/bin/www"
  },
  "dependencies": {
    "body-parser": "latest",
    "compression": "latest",
    "cookie-parser": "latest",
    "cors": "latest",
    "debug": "latest",
    "express": "latest",
    "helmet": "latest",
    "http-status": "latest",
    "lodash": "latest",
    "morgan": "latest",
    "sequelize": "unstable",
    "sqlite3": "latest"
  },
  "devDependencies": {
    "@types/body-parser": "latest",
    "@types/chai": "latest",
    "@types/compression": "latest",
    "@types/cookie-parser": "latest",
    "@types/cors": "latest",
    "@types/debug": "latest",
    "@types/express": "latest",
    "@types/helmet": "latest",
    "@types/http-status": "latest",
    "@types/lodash": "latest",
    "@types/mocha": "latest",
    "@types/morgan": "latest",
    "@types/node": "latest",
    "@types/sequelize": "latest",
    "@types/sinon": "latest",
    "@types/sinon-chai": "latest",
    "@types/sqlite3": "latest",
    "chai": "latest",
    "coveralls": "latest",
    "mocha": "latest",
    "nodemon": "latest",
    "nyc": "latest",
    "rimraf": "latest",
    "sinon": "latest",
    "sinon-chai": "latest",
    "ts-node": "latest",
    "tslint": "latest",
    "typescript": "latest"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 6",
    "npm": ">= 4",
    "yarn": "^0.19.0"
  }
}
